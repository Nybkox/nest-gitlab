{
	"type": "module",
	"name": "@nybkox/nest-gitlab",
	"version": "1.0.1",
	"description": "NestJS wrapper for gitbeaker GitLab API client",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Nybkox/nest-gitlab.git"
	},
	"bugs": {
		"url": "https://github.com/Nybkox/nest-gitlab/issues"
	},
	"homepage": "https://github.com/Nybkox/nest-gitlab#readme",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"files": [
		"dist",
		"README.md"
	],
	"scripts": {
		"lint": "biome lint",
		"lint:fix": "biome lint --write",
		"format": "biome format --write",
		"format:check": "biome format",
		"typescript": "tsc --noEmit",
		"build": "tsdown",
		"prepublishOnly": "pnpm run build"
	},
	"keywords": [
		"nestjs",
		"gitlab",
		"gitbeaker",
		"api"
	],
	"author": "Pavel Sheida nybkox@gmail.com",
	"license": "MIT",
	"packageManager": "pnpm@10.24.0",
	"devDependencies": {
		"@biomejs/biome": "2.3.8",
		"@gitbeaker/rest": "^43.8.0",
		"@nestjs/common": "^11.1.9",
		"@types/node": "^24.10.1",
		"tsdown": "0.17.0-beta.5",
		"typescript": "^5.9.3"
	},
	"peerDependencies": {
		"@gitbeaker/rest": "^43.8.0",
		"@nestjs/common": "^11.1.9"
	}
}
